<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="19003" id="at.florian_lentsch.expirysync" version="1.9" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Expiry Sync</name>
    <description>Expiry Sync</description>
    <author email="office@florian-lentsch.at" href="http://florian-lentsch.at/">Florian Lentsch</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="ScrollEnabled" value="false" />
    <preference name="android-minSdkVersion" value="19" />
    <preference name="BackupWebStorage" value="none" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="FadeSplashScreenDuration" value="300" />
    <preference name="SplashShowOnlyFirstTime" value="true" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="0" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="ldpi" src="resources/android/icon/drawable-ldpi-icon.png" />
        <icon density="mdpi" src="resources/android/icon/drawable-mdpi-icon.png" />
        <icon density="hdpi" src="resources/android/icon/drawable-hdpi-icon.png" />
        <icon density="ldpi" src="resources/android/icon/drawable-ldpi-icon.png" />
        <icon density="xxhdpi" src="resources/android/icon/drawable-xxhdpi-icon.png" />
        <icon density="xxxhdpi" src="resources/android/icon/drawable-xxxhdpi-icon.png" />
    </platform>
    <plugin name="cordova-plugin-whitelist" spec="1.3.3" />
    <plugin name="cordova-plugin-statusbar" spec="2.4.2" />
    <plugin name="cordova-plugin-device" spec="2.0.2" />
    <plugin name="cordova-plugin-splashscreen" spec="5.0.2" />
    <plugin name="cordova-plugin-ionic-webview" spec="^4.0.0" />
    <plugin name="cordova-plugin-ionic-keyboard" spec="^2.0.5" />
    <plugin name="cordova-plugin-app-version" spec="~0.1.9" />
    <plugin name="cordova-plugin-camera" spec="~4.1.0" />
    <plugin name="cordova-plugin-background-mode" spec="~0.7.3" />
    <plugin name="cordova-plugin-shared-preferences" spec="~0.0.1" />
    <plugin name="com-darryncampbell-cordova-plugin-intent" spec="~1.1.8" />
    <plugin name="org.nypr.cordova.wakeupplugin" spec="git://github.com/lentschi/cordova-plugin-wakeuptimer.git#ae50510c53136d90445f3281cafb6908bfbf5ae3" />
    <plugin name="cordova-sqlite-ext" spec="~3.0.0" />
    <plugin name="de.appplant.cordova.plugin.local-notification" spec="git://github.com/katzer/cordova-plugin-local-notifications#a75c0979ceab65b519b53c28047194ce1b82f324" />
    <plugin name="phonegap-plugin-barcodescanner" spec="8.0.1">
        <variable name="ANDROID_SUPPORT_V4_VERSION" value="27.+" />
    </plugin>
    <plugin name="cordova-plugin-app-exit" spec="0.0.1" />
    <plugin name="cordova-plugin-cszbar" spec="git://github.com/lentschi/csZBar.git#b13870816b24c788f2222ebf814c4fd46871608d" />
</widget>
